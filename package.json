{
  "name": "mc-cli",
  "version": "0.0.0",
  "author": "Matt Overall @MatthewOverall",
  "bin": {
    "mc": "./bin/run"
  },
  "bugs": "https://github.com/MatthewOverall/mc-cli/issues",
  "engines": {
    "node": ">=8.5.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/MatthewOverall/mc-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "instance": {
        "description": "manage instances"
      }
    },
    "helpClass": "./lib/help",
    "bin": "mc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "MatthewOverall/mc-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "@types/ws": "^7.2.7",
    "async-retry": "^1.3.1",
    "discord.js": "^12.3.1",
    "inquirer": "^7.3.3",
    "rcon-client": "^4.2.3",
    "shelljs": "^0.8.4",
    "tslib": "^1.14.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.7",
    "@types/async-retry": "^1.4.2",
    "@types/chai": "^4.2.13",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.37",
    "@types/shelljs": "^0.8.8",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
